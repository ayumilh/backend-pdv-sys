{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/?",
      "dest": "api/index.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.ts"
    }
  ],
  "buildCommand": "npm run prisma:generate && npm run build"
}